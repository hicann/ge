<?xml version="1.0" encoding="UTF-8"?>
<config name="ge-compiler" copy_type="delivery" src_path="lib/" dst_path="ge-compiler" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>ge-compiler</func_name>
        <suffix>run</suffix>
        <install_script>share/info/ge-compiler/script/install.sh</install_script>
        <help>share/info/ge-compiler/script/help.info</help>
        <version>package/ge-compiler/version.xml</version>
        <cleanup>share/info/ge-compiler/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
    </package_info>
    <generate_info value="ge-compiler_version.h" dst_path="share/info/ge-compiler" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
       <GE_COMPILER_VERSION>$(ASCEND_VER)</GE_COMPILER_VERSION>
       <GE_COMPILER_TIMESTAMP>$(TIMESTAMP_NO)</GE_COMPILER_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="share/info/ge-compiler" install_path="share/info/ge-compiler" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <version_info install="true" install_path="ge-compiler" install_own="$username:$usergroup" install_mod="440"/>
    <file_info value="ge-compiler_script" copy_type="source" src_path="" dst_path="share/info/ge-compiler" install_path="share/info/ge-compiler">
        <file value="script" entity="true"/>
        <file value="version.info"/>
    </file_info>
    <block_info dependtree="false" dst_path="ge-compiler" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
        <block name="GeCompilerSo"/>
        <block name="GeCompilerInc"/>
        <block name="GeCompilerBin"/>
        <block name="LlmDatadistCompatiableInc"/>
    </block_info>
    <dir_info value="ge-compiler" install_path="ge-compiler" install_type="all">
        <path value="compiler" install_mod="750"/>
        <path value="share" install_mod="750"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/ge-compiler" install_mod="750"/>
        <path value="share/info/ge-compiler/script" install_mod="550"/>
        <path value="$(TARGET_ENV)/devlib" install_softlink="devlib"/>
        <path value="$(TARGET_ENV)/python" install_softlink="compiler/python"/>
    </dir_info>
</config>
